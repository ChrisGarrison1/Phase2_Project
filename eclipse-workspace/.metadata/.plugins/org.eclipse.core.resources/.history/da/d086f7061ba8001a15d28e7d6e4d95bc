package com.annotations;

import org.testng.annotations.*;
public class TestAnnotations{
@Test
	public void Test1() {
		System.out.println("test login to fb via mozillia");
	}
	
	@Test
	public void Test2() {
		System.out.println("test Login to fb via chrme");
	}
	
	@BeforeTest
	public void beforeTest() {
		System.out.println("before test launch browser");
	}
	
	@AfterTest
	public void afterTest() {
		System.out.println("After test close browser");
	}

	@BeforeMethod
	public void beforeMethod() {
		System.out.println("befire meth Launch browser");
	}
	
	@AfterMethod
	public void afterMethod() {
		System.out.println("after meth close browser");
	}
	
	
	@BeforeClass
	public void beforeClass() {
		System.out.println("before CLass launch");
	}
	
	@AfterClass
	public void afterClass() {
		System.out.println("after class 1");
	}

}
